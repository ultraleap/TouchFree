<Window x:Class="TouchFree.Tooling.Wpf.Example.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TouchFree.Tooling.Wpf.Example"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="TopLeftButton" Content="Top Left Button" HorizontalAlignment="Left" Margin="31,23,0,0" VerticalAlignment="Top" Height="103" Width="123" Click="TopLeftButton_Clicked"/>
        <Button x:Name="BottomRightButton" Content="Bottom Right Button" HorizontalAlignment="Left" Margin="461,243,0,0" VerticalAlignment="Top" Height="121" Width="174" Click="BottomRightButton_Clicked"/>
        <TextBox x:Name="ATextBox" HorizontalAlignment="Left" Height="137" Margin="352,58,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="327"/>
        <TextBox x:Name="InfoBox" HorizontalAlignment="Left" Height="73" Margin="31,336,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="285"/>
        <Rectangle x:Name="PositionRectangle" HorizontalAlignment="Left" Height="20" Margin="185,195,0,0" Stroke="Black" VerticalAlignment="Top" Width="20" Fill="#FFC73636" RadiusX="10" RadiusY="10" Visibility="Visible"/>

    </Grid>
</Window>
